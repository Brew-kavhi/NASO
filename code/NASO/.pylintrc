[MASTER]
load-plugins=pylint_django
django-settings-module=naso.settings
ignore=migrations,celery, sample_plugins, pyproject.toml, INIT.sh, INSTALL.sh, README.md, CONTRIBUTE.md, net.log, poetry.lock, requirements.txt, db.sqlite3
min-similarity-lines=5

[FORMAT]
max-line-length=130

[MESSAGES CONTROL]
disable=missing-docstring

[DESIGN]
max-parents=13

[REPORTS]
output-format = colorized

[TYPECHECK]
generated-members=REQUEST,acl_users,aq_parent,"[a-zA-Z]+_set{1,2}",save,delete
init-import=no
