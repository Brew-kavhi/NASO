[MASTER]
load-plugins=pylint_django
django-settings-module=naso.settings_local
ignore=migrations

[FORMAT]
max-line-length=120

[MESSAGES CONTROL]
disable=missing-docstring

[DESIGN]
max-parents=13

[REPORTS]
output-format = colorized

[TYPECHECK]
generated-members=REQUEST,acl_users,aq_parent,"[a-zA-Z]+_set{1,2}",save,delete
init-import=no
